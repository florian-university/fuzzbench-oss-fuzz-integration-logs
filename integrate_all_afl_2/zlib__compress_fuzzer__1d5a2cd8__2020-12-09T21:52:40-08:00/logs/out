source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/zlib_compress_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/zlib_compress_fuzzer \
--file benchmarks/zlib_compress_fuzzer/Dockerfile \
benchmarks/zlib_compress_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/zlib_compress_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/zlib_compress_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/zlib_compress_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
