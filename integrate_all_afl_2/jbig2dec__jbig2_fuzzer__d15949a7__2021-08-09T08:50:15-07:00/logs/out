source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/jbig2dec_jbig2_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/jbig2dec_jbig2_fuzzer \
--file benchmarks/jbig2dec_jbig2_fuzzer/Dockerfile \
benchmarks/jbig2dec_jbig2_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/jbig2dec_jbig2_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/jbig2dec_jbig2_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/jbig2dec_jbig2_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
