docker build \
--tag gcr.io/fuzzbench/builders/afl/jbig2dec_jbig2_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/jbig2dec_jbig2_fuzzer \
--build-arg benchmark=jbig2dec_jbig2_fuzzer \
--build-arg fuzzer=afl \
