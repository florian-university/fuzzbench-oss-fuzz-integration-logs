source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/znc_msg_parse_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/znc_msg_parse_fuzzer \
--file benchmarks/znc_msg_parse_fuzzer/Dockerfile \
benchmarks/znc_msg_parse_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/znc_msg_parse_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/znc_msg_parse_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/znc_msg_parse_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
