source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/bzip2_bzip2_decompress_target \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/bzip2_bzip2_decompress_target \
--file benchmarks/bzip2_bzip2_decompress_target/Dockerfile \
benchmarks/bzip2_bzip2_decompress_target
docker build \
--tag gcr.io/fuzzbench/builders/afl/bzip2_bzip2_decompress_target-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/bzip2_bzip2_decompress_target-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/bzip2_bzip2_decompress_target \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
