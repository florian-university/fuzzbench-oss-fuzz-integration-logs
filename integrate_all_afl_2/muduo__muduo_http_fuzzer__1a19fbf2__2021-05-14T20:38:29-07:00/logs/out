source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/muduo_muduo_http_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/muduo_muduo_http_fuzzer \
--file benchmarks/muduo_muduo_http_fuzzer/Dockerfile \
benchmarks/muduo_muduo_http_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/muduo_muduo_http_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/muduo_muduo_http_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/muduo_muduo_http_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
xenial: Pulling from library/ubuntu
