source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/gnupg_fuzz_import \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/gnupg_fuzz_import \
--file benchmarks/gnupg_fuzz_import/Dockerfile \
benchmarks/gnupg_fuzz_import
docker build \
--tag gcr.io/fuzzbench/builders/afl/gnupg_fuzz_import-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/gnupg_fuzz_import-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/gnupg_fuzz_import \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
