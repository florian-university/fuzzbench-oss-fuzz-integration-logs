source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/boost_boost_ptree_xmlread_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/boost_boost_ptree_xmlread_fuzzer \
--file benchmarks/boost_boost_ptree_xmlread_fuzzer/Dockerfile \
benchmarks/boost_boost_ptree_xmlread_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/boost_boost_ptree_xmlread_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/boost_boost_ptree_xmlread_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/boost_boost_ptree_xmlread_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
xenial: Pulling from library/ubuntu
