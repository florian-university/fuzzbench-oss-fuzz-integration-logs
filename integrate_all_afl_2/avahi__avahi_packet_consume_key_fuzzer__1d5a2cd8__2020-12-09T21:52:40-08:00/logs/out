source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/avahi_avahi_packet_consume_key_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/avahi_avahi_packet_consume_key_fuzzer \
--file benchmarks/avahi_avahi_packet_consume_key_fuzzer/Dockerfile \
benchmarks/avahi_avahi_packet_consume_key_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/avahi_avahi_packet_consume_key_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/avahi_avahi_packet_consume_key_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/avahi_avahi_packet_consume_key_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
