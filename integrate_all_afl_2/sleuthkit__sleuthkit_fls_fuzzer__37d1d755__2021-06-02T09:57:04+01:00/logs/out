source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/sleuthkit_sleuthkit_fls_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/sleuthkit_sleuthkit_fls_fuzzer \
--file benchmarks/sleuthkit_sleuthkit_fls_fuzzer/Dockerfile \
benchmarks/sleuthkit_sleuthkit_fls_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/sleuthkit_sleuthkit_fls_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/sleuthkit_sleuthkit_fls_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/sleuthkit_sleuthkit_fls_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
