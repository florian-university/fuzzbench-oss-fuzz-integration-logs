source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/gnupg_fuzz_decrypt \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/gnupg_fuzz_decrypt \
--file benchmarks/gnupg_fuzz_decrypt/Dockerfile \
benchmarks/gnupg_fuzz_decrypt
docker build \
--tag gcr.io/fuzzbench/builders/afl/gnupg_fuzz_decrypt-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/gnupg_fuzz_decrypt-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/gnupg_fuzz_decrypt \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
