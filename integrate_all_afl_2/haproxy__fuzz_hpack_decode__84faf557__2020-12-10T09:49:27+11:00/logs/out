source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/haproxy_fuzz_hpack_decode \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/haproxy_fuzz_hpack_decode \
--file benchmarks/haproxy_fuzz_hpack_decode/Dockerfile \
benchmarks/haproxy_fuzz_hpack_decode
docker build \
--tag gcr.io/fuzzbench/builders/afl/haproxy_fuzz_hpack_decode-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/haproxy_fuzz_hpack_decode-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/haproxy_fuzz_hpack_decode \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
