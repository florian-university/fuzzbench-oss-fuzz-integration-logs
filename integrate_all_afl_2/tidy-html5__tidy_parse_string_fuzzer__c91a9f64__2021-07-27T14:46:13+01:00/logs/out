docker build \
--tag gcr.io/fuzzbench/builders/benchmark/tidy-html5_tidy_parse_string_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/tidy-html5_tidy_parse_string_fuzzer \
--file benchmarks/tidy-html5_tidy_parse_string_fuzzer/Dockerfile \
benchmarks/tidy-html5_tidy_parse_string_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/tidy-html5_tidy_parse_string_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/tidy-html5_tidy_parse_string_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/tidy-html5_tidy_parse_string_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
