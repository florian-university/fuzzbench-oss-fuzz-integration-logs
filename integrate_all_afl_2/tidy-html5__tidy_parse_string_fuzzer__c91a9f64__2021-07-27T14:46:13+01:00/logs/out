--tag gcr.io/fuzzbench/builders/afl/tidy-html5_tidy_parse_string_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/tidy-html5_tidy_parse_string_fuzzer \
--build-arg benchmark=tidy-html5_tidy_parse_string_fuzzer \
