--tag gcr.io/fuzzbench/base-image \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/base-image \
--file docker/base-image/Dockerfile \
.
docker build \
--tag gcr.io/fuzzbench/builders/afl/lighttpd_fuzz_burl \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/lighttpd_fuzz_burl \
--build-arg benchmark=lighttpd_fuzz_burl \
--build-arg fuzzer=afl \
--build-arg parent_image=gcr.io/fuzzbench/builders/afl/lighttpd_fuzz_burl-intermediate \
--file docker/benchmark-builder/Dockerfile \
.
