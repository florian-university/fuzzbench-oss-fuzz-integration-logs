source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/lighttpd_fuzz_burl \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/lighttpd_fuzz_burl \
--file benchmarks/lighttpd_fuzz_burl/Dockerfile \
benchmarks/lighttpd_fuzz_burl
docker build \
--tag gcr.io/fuzzbench/builders/afl/lighttpd_fuzz_burl-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/lighttpd_fuzz_burl-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/lighttpd_fuzz_burl \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
