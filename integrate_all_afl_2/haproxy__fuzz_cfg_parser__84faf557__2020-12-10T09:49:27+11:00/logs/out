source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/haproxy_fuzz_cfg_parser \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/haproxy_fuzz_cfg_parser \
--file benchmarks/haproxy_fuzz_cfg_parser/Dockerfile \
benchmarks/haproxy_fuzz_cfg_parser
docker build \
--tag gcr.io/fuzzbench/builders/afl/haproxy_fuzz_cfg_parser-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/haproxy_fuzz_cfg_parser-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/haproxy_fuzz_cfg_parser \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
docker pull ubuntu:xenial
