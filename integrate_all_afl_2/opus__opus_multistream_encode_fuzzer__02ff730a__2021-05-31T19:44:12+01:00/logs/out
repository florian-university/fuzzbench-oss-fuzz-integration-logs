source .venv/bin/activate && PYTHONPATH=. python3 docker/generate_makefile.py docker/generated.mk
docker build \
--tag gcr.io/fuzzbench/builders/benchmark/opus_opus_multistream_encode_fuzzer \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/benchmark/opus_opus_multistream_encode_fuzzer \
--file benchmarks/opus_opus_multistream_encode_fuzzer/Dockerfile \
benchmarks/opus_opus_multistream_encode_fuzzer
docker build \
--tag gcr.io/fuzzbench/builders/afl/opus_opus_multistream_encode_fuzzer-intermediate \
--build-arg BUILDKIT_INLINE_CACHE=1 \
--cache-from gcr.io/fuzzbench/builders/afl/opus_opus_multistream_encode_fuzzer-intermediate \
--build-arg parent_image=gcr.io/fuzzbench/builders/benchmark/opus_opus_multistream_encode_fuzzer \
--file fuzzers/afl/builder.Dockerfile \
fuzzers/afl
